<p id="notice"><%= notice %></p>

<h1>Listing Direccions</h1>

<table>
  <thead>
    <tr>
      <th>Cli cod</th>
      <th>Guia desp cod</th>
      <th>Dir calle</th>
      <th>Dir numero</th>
      <th>Dir comuna</th>
      <th>Dir ciudad</th>
      <th>Dir tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @direccions.each do |direccion| %>
      <tr>
        <td><%= direccion.cli_cod %></td>
        <td><%= direccion.guia_desp_cod %></td>
        <td><%= direccion.dir_calle %></td>
        <td><%= direccion.dir_numero %></td>
        <td><%= direccion.dir_comuna %></td>
        <td><%= direccion.dir_ciudad %></td>
        <td><%= direccion.dir_tipo %></td>
        <td><%= link_to 'Show', direccion %></td>
        <td><%= link_to 'Edit', edit_direccion_path(direccion) %></td>
        <td><%= link_to 'Destroy', direccion, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Direccion', new_direccion_path %>
