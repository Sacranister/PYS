<p id="notice"><%= notice %></p>

<h1>Listing Detalle Pedidos</h1>

<table>
  <thead>
    <tr>
      <th>Ped cod</th>
      <th>Det ped linea</th>
      <th>Ins cod</th>
      <th>Ins cod prov</th>
      <th>Det ped cant</th>
      <th>Det ped precio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @detalle_pedidos.each do |detalle_pedido| %>
      <tr>
        <td><%= detalle_pedido.ped_cod %></td>
        <td><%= detalle_pedido.det_ped_linea %></td>
        <td><%= detalle_pedido.ins_cod %></td>
        <td><%= detalle_pedido.ins_cod_prov %></td>
        <td><%= detalle_pedido.det_ped_cant %></td>
        <td><%= detalle_pedido.det_ped_precio %></td>
        <td><%= link_to 'Show', detalle_pedido %></td>
        <td><%= link_to 'Edit', edit_detalle_pedido_path(detalle_pedido) %></td>
        <td><%= link_to 'Destroy', detalle_pedido, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Detalle pedido', new_detalle_pedido_path %>
