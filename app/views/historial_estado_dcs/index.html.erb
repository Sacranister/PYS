 

<h1>Listing Historial Estado Dcs</h1>

<table>
  <thead>
    <tr>
      <th>Doc com cod</th>
      <th>Est dc cod</th>
      <th>Hist est dc cod</th>
      <th>Hist est dc fecha</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @historial_estado_dcs.each do |historial_estado_dc| %>
      <tr>
        <td><%= historial_estado_dc.doc_com_cod %></td>
        <td><%= historial_estado_dc.est_dc_cod %></td>
        <td><%= historial_estado_dc.hist_est_dc_cod %></td>
        <td><%= historial_estado_dc.hist_est_dc_fecha %></td>
        <td><%= link_to 'Show', historial_estado_dc %></td>
        <td><%= link_to 'Edit', edit_historial_estado_dc_path(historial_estado_dc) %></td>
        <td><%= link_to 'Destroy', historial_estado_dc, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

